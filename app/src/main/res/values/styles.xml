        <item name="android:layout_height">@dimen/_60sdp</item>
    </style>
    <style name="dialogs_list_photo_size">
        <item name="android:layout_width">@dimen/_40sdp</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
        <item name="android:button">@null</item>
    </style>
    <style name="dialog_photo">
        <item name="android:layout_width">@dimen/_75sdp</item>
        <item name="android:layout_height">@dimen/_75sdp</item>
        <item name="android:button">@null</item>
    </style>
    <style name="filter_radio_button">
        <item name="android:layout_width">@dimen/filter_radio_button_size</item>
        <item name="android:layout_height">@dimen/filter_radio_button_size</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style name="vip_photo_size">
        <item name="android:layout_width">@dimen/_50sdp</item>
        <item name="android:layout_height">@dimen/_50sdp</item>
    </style>
    <style name="setting_notification_button_size">
        <item name="android:layout_width">@dimen/settings_notification_item_size</item>
        <item name="android:layout_height">@dimen/settings_notification_item_size</item>
    </style>


    <style name="verify_photo_style">
        <item name="android:textColor">@color/orange</item>
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="PaymentSettingsMargin">
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginStart">@dimen/_43sdp</item>
        <item name="android:layout_marginLeft">@dimen/_43sdp</item>
        <item name="android:layout_marginRight">@dimen/_43sdp</item>
        <item name="android:layout_marginEnd">@dimen/_43sdp</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="textViewSize">
        <item name="android:textColor">@color/text_main</item>
        <item name="android:textSize">@dimen/app_font_size</item>
    </style>

    <style name="filter_right_margin">
        <item name="android:layout_marginEnd">@dimen/_10sdp</item>
        <item name="android:layout_marginRight">@dimen/_10sdp</item>
    </style>

    <style name="menuLeftMargin" parent="margined">
        <item name="android:layout_marginLeft">@dimen/_18sdp</item>
        <item name="android:layout_marginStart">@dimen/_18sdp</item>
    </style>

    <style name="menuRightMargin">
        <item name="android:layout_marginLeft">@dimen/_10sdp</item>
