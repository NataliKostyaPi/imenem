<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/profile_container">
    <LinearLayout
        android:id="@+id/fragmentTitle"
        style="@style/profile_header_layout">

        <ImageView
            style="@style/profile_element_logo"
            android:src="@drawable/interes"
            />
        <TextView
            android:text="@string/interests"
            android:id="@+id/friendsCountText"
            style="@style/profile_element_title" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:gravity="end">
            <TextView
                android:id="@+id/changeButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/profile_elements_logontext_margin"
                android:layout_marginStart="@dimen/profile_elements_logontext_margin"
                android:text="@string/change"
                android:textColor="@color/orange_text"
                android:textSize="@dimen/app_font_size" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/dataContainer"
        style="@style/profile_data_container">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/interestsLayout"
        >



    </LinearLayout>
    </LinearLayout>

    <ImageView style="@style/profile_divider" />




</RelativeLayout>